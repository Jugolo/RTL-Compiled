if(typeof tcp_readln != "function")
  include "rtl.io.tcp.client";
if(typeof hashmap != "function")
	include "rtl.hashmap";


function HASHMAP parseHTTPHeader(t){
	h = hashmap();
	while((line = tcp_readln(t)) != ""){
		pos = strpos(line, ": ");
		if(pos == -1)
			return h;
  h._struct.set(substr(line, 0, pos), substr(line, pos+2));
	}
	return h;
}
